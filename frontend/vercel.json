{
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": { "distDir": "dist" }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "https://mern-chatbot-ai-ruddy.vercel.app/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}
